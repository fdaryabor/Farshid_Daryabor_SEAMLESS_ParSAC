    <target name="depth_of_chl_max_in_10m_to_45m" expression="(msi_ergom1_pp+msi_ergom1_ff+msi_ergom1_bb)[:,:45].mean(axis=0).argmax(axis=0)" path="result.nc"/>
    <target name="ocean_colour" expression="(((msi_ergom1_pp+msi_ergom1_ff+msi_ergom1_bb)*h)[:,:].sum(axis=1)/h[:,:].sum(axis=1)).mean(axis=0)" path="result.nc"/>
    <target name="BGC_Argo_chl_0m_to_45m" expression="(((msi_ergom1_pp+msi_ergom1_ff+msi_ergom1_bb)*h)[:,:].sum(axis=1)/h[:,:].sum(axis=1)).mean(axis=0)" path="result.nc"/>
    <target name="BGC_Argo_no3_0m_to_45m" expression="(((msi_ergom1_nn)*h)[:,:].sum(axis=1)/h[:,:].sum(axis=1)).mean(axis=0)" path="result.nc"/>
    <target name="BGC_Argo_po4_0m_to_45m" expression="(((msi_ergom1_po)*h)[:,:].sum(axis=1)/h[:,:].sum(axis=1)).mean(axis=0)" path="result.nc"/>
    <target name="BGC_Argo_POC_0m_to_45m" expression="(((msi_ergom1_dd+msi_ergom1_ff+msi_ergom1_bb)*h)[:,:].sum(axis=1)/h[:,:].sum(axis=1)).mean(axis=0)" path="result.nc"/>
